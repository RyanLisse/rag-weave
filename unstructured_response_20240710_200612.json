    {
        "type": "NarrativeText",
        "element_id": "c254e8c3fd0495225c85a597c64d21f8",
        "text": "Whenever there is a problem on your RoboRail machine, our technical support team is always ready to help you. Our team has several diagnostic tools at their disposal. To help you successfully it is very important that the following information is as complete as possible.",
        "metadata": {
            "filetype": "application/pdf",
            "languages": [
                "eng"
            ],
            "page_number": 1,
            "parent_id": "5b51a3676f82dac4b7511cbb996db2a9",
            "filename": "FAQ Data collection.pdf"
        }
    },
    {
        "type": "NarrativeText",
        "element_id": "652c7a59fde4d86df0fcb78a528738c0",
